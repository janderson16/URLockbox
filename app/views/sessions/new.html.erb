<h1>Welcome to URLockbox</h1><br>
<h3>Log In or Sign Up</h3><br>
<h4> <%= link_to "Sign up", new_user_path %> </h4>

<%= form_for :session, url: login_path do |f| %>

<%= f.label :email %>
<%= f.text_field :email %><br>

<%= f.label :password %>
<%= f.password_field :password %><br><br>

<%= f.submit "Login" %>
<% end %>
